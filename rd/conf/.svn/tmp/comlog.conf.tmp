#进程名
COMLOG_PROCNAME : rls
COMLOG_LEVEL : 4
#支持的设备数目
COMLOG_DEVICE_NUM : 1
#支持的自定义等级
COMLOG_SELFDEFINE : MESSAGE,BPR

#支持的设备名
#这里有一个FILE，那么后面就有对应的FILE_XXX的配置
COMLOG_DEVICE0 : ULLOG

#device 1: ULLOG
ULLOG_TYPE : ULLOG
ULLOG_NAME : rls.log
ULLOG_PATH : ./log
ULLOG_SYSLEVEL : 4
ULLOG_OPEN : 1
#默认的SPLITE_TYPE是TRUNCT格式，即超过某个大小直接回滚
ULLOG_SIZE : 2000
ULLOG_SELFLEVEL : MESSAGE
